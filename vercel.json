{
  "rewrites": [
    {
      "source": "/api/admin-logout",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin-update-application",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/ambassador-login",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/ambassador-application",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/ambassadors/active",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/phone-subscribe",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/send-email",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/passes/:eventId",
      "destination": "/api/passes-[eventId].js"
    },
    {
      "source": "/api/orders/create",
      "destination": "/api/orders-create.js"
    },
    {
      "source": "/api/aio-events/save-submission",
      "destination": "/api/aio-events-save-submission.js"
    },
    {
      "source": "/api/admin/passes/:eventId",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/passes/:id/stock",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/passes/:id/payment-methods",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/passes/:id/activate",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin-skip-ambassador-confirmation",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin-resend-ticket-email",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin-remove-order",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/update-order-email",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/aio-events-submissions",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/ambassador-sales/orders",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/order-expiration-settings",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/set-order-expiration",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/clear-order-expiration",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/auto-reject-expired-orders",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/ambassador/orders",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/ambassador/performance",
      "destination": "/api/misc.js"
    },
    {
      "source": "/api/admin/logs",
      "destination": "/api/admin/logs.js"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/admin-login.js": {
      "maxDuration": 30
    },
    "api/admin-approve-order.js": {
      "maxDuration": 30
    },
    "api/verify-admin.js": {
      "maxDuration": 30
    },
    "api/orders-create.js": {
      "maxDuration": 60
    },
    "api/aio-events-save-submission.js": {
      "maxDuration": 30
    },
    "api/passes-[eventId].js": {
      "maxDuration": 30
    },
    "api/misc.js": {
      "maxDuration": 60
    },
    "api/admin/logs.js": {
      "maxDuration": 60
    }
  }
}
